block variables

doctype html
html
	head
		meta(charset="utf-8")
		title #{title}
		meta(content="" name="description")
		meta(content="" name="keywords")
		meta(name="viewport" content="width=device-width, initial-scale=1")
		meta(content="ie=edge" http-equiv="x-ua-compatible")
		link(rel="shortcut icon" href="assets/images/favicon.ico" type="image/x-icon")
		link(href="https://fonts.googleapis.com/css?family=Montserrat:400,500,600,700&display=swap" rel="stylesheet")
		link(rel="stylesheet" type="text/css" href="assets/scripts/aos-master/dist/aos.css")
		link(rel="stylesheet", href="assets/styles/app.min.css")
		
	body
		.wrapper
			include common/header.pug
			.content
				block content
			include common/footer.pug 

		script(src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js")
		script(src="assets/scripts/jquery.inputmask.bundle.js")
		script(src="assets/scripts/aos-master/dist/aos.js")
		script(src="assets/scripts/masonry.js")
		script(src="assets/scripts/main.js")
